# your code goes here
class Stack():
	def __init__ (self):
		self.items = []
		
	def push(self, item):
		self.items.append(item)
		
	def pop(self):
		return self.items.pop()
	
	def is_empty(self):
		return self.items == []
		
	def get_stack(self):
		return self.items

		
s = Stack()
s.push('A')
s.push('B')

print(s.get_stack())

s.push('C')
print(s.get_stack())

print(s.pop())

print(s.get_stack())


c= Stack()

c.push('aaa')

print(c.get_stack())

print(c.is_empty())
